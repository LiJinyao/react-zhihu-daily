@import "normalize.css";
@import "../statics/consts"
@font-face
  font-family iconfont
  font-style normal
  src url('../statics/iconfont/iconfont.eot')
  src url('../statics/iconfont/iconfont.woff') format('woff'),
      url('../statics/iconfont/iconfont.ttf') format('truetype'),
      url('../statics/iconfont/iconfont.svg') format('svg')
:global
  .iconfont
    font-family: iconfont !important;
    color: inherit;
    font-style:normal;
    -webkit-font-smoothing: antialiased;
    -webkit-text-stroke-width: 0.2px;
    -moz-osx-font-smoothing: grayscale;
  .mainContainer
    width: mainBody;
    margin: 0 auto;
    @media screen and (max-width: 700px)
      width: 100%;
body
  background-color: bkgMain;
  font-family: 'Helvetica Neue',Helvetica,Arial,sans-serif;
a
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0)
