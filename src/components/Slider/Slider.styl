// 640px
.sliderWarp
  width: 100%;
  height: 400px;
  position: relative;
.sliderBody
  height: 400px;
  width: 640px;
  margin: 0 auto;
  padding: 0;
  list-style: none;
  box-sizing: border-box;
  position: relative;
  overflow: hidden;
.slideEnter
  opacity: 0.01;
.slideEnterActive
  opacity: 1;
  transition: opacity .5s ease-in;
