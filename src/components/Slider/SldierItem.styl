.itemwarp
  height: inherit;
  width: 100%;
  display: none;
  position: relative;
  box-sizing: border-box;

.active
  display: block;
  transform: translateX(0);
.right
.next
  display: block;
  position: absolute;
  top: 0;
  transform: translateX(100%);
.left
.prev
  display: block;
  position: absolute;
  top: 0;
  transform: translateX(-100%);
.linkwarp
  position: absolute;
  display: block;
  height: 100%;
  width: 100%;
